{
  "name": "enfujo-www",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "lint:fix": "yarn lint --fix",
    "serve": "npm run server:start",
    "stop": "npm run server:stop",
    "restart": "npm run server:restart",
    "server:start": "npm run build && pm2 start ecosystem.config.js",
    "server:stop": "pm2 stop ecosystem.config.js",
    "server:restart": "pm2 restart ecosystem.config.js"
  },
  "dependencies": {
    "core-js": "^3.13.1",
    "nuxt": "^2.15.6"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.10.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "nuxt-graphql-request": "^4.0.0",
    "prettier": "^2.3.0",
    "sass": "^1.34.0",
    "sass-loader": "10"
  }
}
